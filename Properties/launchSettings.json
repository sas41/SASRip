{
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "SASRip": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "KeepFiles": "false",
        "EnableVerboseLogging": "true"
      },
      "applicationUrl": "https://localhost:54101;http://localhost:54100"
    },
    "Dockerfile HTTP": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
        "environmentVariables": {
            "KeepFiles": "true",
            "EnableVerboseLogging": "true",
            "ASPNETCORE_ENVIRONMENT": "Docker",
            "ASPNETCORE_HTTP_PORT": "54100",
            "ASPNETCORE_URLS": "http://+:54100"
        },
      "publishAllPorts": true,
      "useSSL": false,
      "httpPort": 54100
    },
    "Dockerfile SSL": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "KeepFiles": "true",
        "EnableVerboseLogging": "true",
        "ASPNETCORE_ENVIRONMENT": "Docker",
        "ASPNETCORE_HTTPS_PORT": "54101",
        "ASPNETCORE_HTTP_PORT": "54100",
        "ASPNETCORE_URLS": "https://+:54101;http://+:54100"
      },
      "publishAllPorts": true,
      "useSSL": true,
      "sslPort": 54101,
      "httpPort": 54100
    }
  },
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:61874/",
      "sslPort": 44387
    }
  }
}